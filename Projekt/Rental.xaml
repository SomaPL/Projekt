<Page x:Class="Projekt.Rental"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Projekt"
      mc:Ignorable="d" 
      d:DesignHeight="450" d:DesignWidth="800"
      Title="Rental">

    <Grid>
        <DataGrid Name="Grid1" AutoGenerateColumns="False">
            <DataGrid.Columns>
                <DataGridTextColumn Binding="{Binding Id}" Header="Id"  MinWidth="100"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding CzyDostepny}" Header="Czy Dostępny" MinWidth="100"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding Marka}" Header="Marka" MinWidth="100"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding Model}" Header="Model" MinWidth="100"></DataGridTextColumn>
                <DataGridTextColumn Binding="{Binding IleOsob}" Header="Ile osobowy" MinWidth="100"></DataGridTextColumn>
            </DataGrid.Columns> 
        </DataGrid>
    </Grid>
</Page>
